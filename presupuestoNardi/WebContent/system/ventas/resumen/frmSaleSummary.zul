							<radio
								checked="@load(vm.saleSummary.intercom)"
								disabled="@load(not vm.editing)" label="Si" value="true">
							</radio>
							<radio
								checked="@load(vm.saleSummary.intercom ? 'false':'true')"
								disabled="@load(not vm.editing)" label="No" value="false">
							</radio>
						</radiogroup>
						<label sclass="formLabel" value="Teléfono:"></label>
						<radiogroup
							selectedItem="@save(vm.saleSummary.phone)">
							<radio checked="@load(vm.saleSummary.phone)"
								disabled="@load(not vm.editing)" label="Si" value="true">
							</radio>
							<radio
								checked="@load(vm.saleSummary.phone ? 'false':'true')"
								disabled="@load(not vm.editing)" label="No" value="false">
							</radio>
						</radiogroup>
						<space></space>
					</row>
					<row>
						<label sclass="formLabel"
							value="Sistema de Acceso:">
						</label>
						<radiogroup
							selectedItem="@save(vm.saleSummary.accessSytem)">
							<radio
								checked="@load(vm.saleSummary.accessSytem)"
								disabled="@load(not vm.editing)" label="Si" value="true">
							</radio>
							<radio
								checked="@load(vm.saleSummary.accessSytem ? 'false':'true')"
								disabled="@load(not vm.editing)" label="No" value="false">
