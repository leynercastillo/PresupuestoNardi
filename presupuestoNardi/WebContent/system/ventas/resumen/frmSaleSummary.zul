						<radiogroup
							selectedItem="@save(vm.saleSummary.firefighterKeychain)">
							<radio
								checked="@load(vm.saleSummary.firefighterKeychain)"
								disabled="@load(not vm.editing)" label="Si" value="true">
							</radio>
							<radio
								checked="@load(vm.saleSummary.firefighterKeychain ? 'false':'true')"
								disabled="@load(not vm.editing)" label="No" value="false">
							</radio>
