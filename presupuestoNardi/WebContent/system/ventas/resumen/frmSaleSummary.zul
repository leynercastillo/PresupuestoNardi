		</groupbox>
		<groupbox>
			<grid>
				<columns>
					<column width="15%"></column>
					<column width="60%"></column>
					<column></column>
				</columns>
				<rows>
					<row>
						<label sclass="formLabel"
							value="Comentarios:">
						</label>
						<cell colspan="2">
							<textbox disabled="@load(vm.searching)"
								readonly="@load(not vm.searching ? not vm.editing : 'false')"
								maxlength="1500" multiline="true" rows="4"
								value="@bind(vm.saleSummary.comment)" width="99%">
							</textbox>
						</cell>
					</row>
				</rows>
			</grid>
		</groupbox>
		<grid>
			<rows>
				<row>
					<div>
						<button disabled="@load(not vm.editing)"
							image="/resource/images/icons/email.png" label="Enviar"
							onClick="@command('confirmSave')">
						</button>
						<button
							disabled="@load(not vm.searching ? vm.editing : 'true')"
							if="${sec:isAnyGranted('ROLE_ITEM_MASTER')}"
							image="/resource/images/icons/edit.png" label="Editar"
							onClick="@command('edit', component = self)">
						</button>
						<button
							disabled="@load(not vm.searching ? vm.editing : 'true')"
							image="/resource/images/icons/print.png" label="Imprimir"
							onClick="@command('print')">
						</button>
						<button
							image="/resource/images/icons/cancel.png" label="Cancelar"
							onClick="@command('restartForm')">
						</button>
					</div>
				</row>
			</rows>
		</grid>
		<window border="normal" mode="modal" title="Atencion"
			visible="@load(not empty vm.modalMessage)" width="25%">
			<vbox hflex="true">
				<hbox height="35%">
					<image src="~./zul/img/msgbox/question-btn.png" />
					<label value="@load(vm.modalMessage)" />
				</hbox>
				<hbox hflex="true" pack="center">
					<button label="Si" onClick="@command('save')" />
					<button label="No"
						onClick="@command('cancelModal')" />
				</hbox>
			</vbox>
		</window>
	</window>
