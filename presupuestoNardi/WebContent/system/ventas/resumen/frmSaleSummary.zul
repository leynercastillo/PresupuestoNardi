							<radio
								checked="@load(vm.saleSummary.gomSystem)"
								disabled="@load(not vm.editing)" label="Si" value="true">
							</radio>
							<radio
								checked="@load(vm.saleSummary.gomSystem ? 'false':'true')"
								disabled="@load(not vm.editing)" label="No" value="false">
							</radio>
