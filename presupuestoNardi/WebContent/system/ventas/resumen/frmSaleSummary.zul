								model="@load(vm.editing ? vm.listFloorDisplay : null)"
								readonly="@load(not vm.searching)"
								selectedItem="@bind(vm.saleSummary.basicDataByFloorDisplay)"
								value="@save(vm.seleccione) @load(vm.saleSummary.basicDataByFloorDisplay eq null ? vm.seleccione : vm.saleSummary.basicDataByFloorDisplay.name)"
