<?taglib uri="http://www.zkoss.org/zkspring/security" prefix="sec"?>
<?variable-resolver class="org.zkoss.spring.DelegatingVariableResolver"?>
<zk>
	<style src="resource/style/styleZK.css"></style>
	<window apply="org.zkoss.bind.BindComposer" border="normal"
		closable="true" onClose="@command('close')"
		viewModel="@id('vm') @init('controller.ventas.resumen.FrmSaleSummary')">
		<caption label="RESUMEN DE VENTA" zclass="formTitleLabel">
		</caption>
		<groupbox closable="false">
			<grid>
				<columns>
					<column width="15%"></column>
					<column width="30%"></column>
					<column width="15%"></column>
					<column width="20%"></column>
					<column></column>
				</columns>
				<rows>
					<row>
						<space></space>
						<space></space>
						<label sclass="formLabel"
							value="Nro Resumen Venta:">
						</label>
						<hlayout>
							<combobox autocomplete="@load(vm.searching)"
								autodrop="@load(vm.searching)" buttonVisible="false" hflex="4"
								model="@load(vm.listNumber)"
								onChange="@command(vm.searching ? 'loadSaleSummary':'', field='number', val=self.value)"
								onFocus="@command(vm.searching ? 'searchSaleSummaryByField':'', field='number')"
								readonly="@load(not vm.searching)"
								value="@load(vm.saleSummary.number)">
							</combobox>
							<intbox disabled="@load(vm.searching)"
								hflex="1" readonly="@load(not vm.searching)"
								value="@load(vm.saleSummary.version)">
							</intbox>
						</hlayout>
						<space></space>
					</row>
					<row>
						<space></space>
						<space></space>
						<label sclass="formLabel"
							value="Nro Presupuesto:">
						</label>
						<hlayout>
							<intbox disabled="@load(vm.searching)"
								hflex="1" readonly="@load(not vm.searching)"
								value="@load(vm.saleSummary.quotation.type ? 1 : vm.saleSummary.quotation.type ne null ? 2 : 0)">
							</intbox>
							<combobox autocomplete="@load(vm.searching)"
								autodrop="@load(vm.searching)" buttonVisible="false" hflex="4"
								model="@load(vm.listQuotationNumber)"
								onChange="@command(vm.searching ? 'loadSaleSummary':'', field='quotationNumber', val=self.value)"
								onFocus="@command(vm.searching ? 'searchSaleSummaryByField':'', field='quotationNumber')"
								readonly="@load(not vm.searching)"
								value="@load(vm.saleSummary.quotation.type ? vm.saleSummary.quotation.newNumber : vm.saleSummary.quotation.modernizationNumber)">
							</combobox>
							<intbox disabled="@load(vm.searching)"
								hflex="1" readonly="@load(not vm.searching)"
								value="@load(vm.saleSummary.quotation.versionNumber)">
							</intbox>
						</hlayout>
						<space></space>
					</row>
					<row>
						<label sclass="formLabel" value="Rif:"></label>
						<hlayout>
							<textbox disabled="@load(vm.searching)"
								hflex="1" readonly="@load(not vm.searching)"
								value="@load(vm.saleSummary.quotation.rifType)">
							</textbox>
							<combobox autocomplete="@load(vm.searching)"
								autodrop="@load(vm.searching)" buttonVisible="false" hflex="4"
								maxlength="15" model="@load(vm.listRifPartner)"
								onChange="@command(vm.searching ? 'loadSaleSummary' : '', field='rifPartner', val=self.value)"
								onFocus="@command(vm.searching ? 'searchSaleSummaryByField' : '', field='rifPartner')"
								readonly="@load(not vm.searching)"
								value="@load(vm.saleSummary.quotation.rifPartner)">
							</combobox>
						</hlayout>
						<label sclass="formLabel"
							value="Fecha Aprobacion:">
						</label>
						<datebox buttonVisible="false"
							disabled="@load(vm.searching)" format="dd/MM/yyyy" hflex="1"
							readonly="@load(not vm.searching)"
							value="@load(vm.saleSummary.quotation.approvedDate)">
						</datebox>
						<space></space>
					</row>
					<row>
						<label sclass="formLabel" value="Cliente:"></label>
						<combobox autocomplete="@load(vm.searching)"
							autodrop="@load(vm.searching)" buttonVisible="false"
							maxlength="100" model="@load(vm.listPartnerName)"
							onChange="@command(vm.searching ? 'loadSaleSummary':'', field='partnerName', val=self.value)"
							onFocus="@command(vm.searching ? 'searchSaleSummaryByField':'', field='partnerName')"
							readonly="@load(not vm.searching)"
							value="@load(vm.saleSummary.quotation.partnerName)"
							width="100%">
						</combobox>
						<label sclass="formLabel"
							value="Fecha Entrega:">
						</label>
						<datebox buttonVisible="@load(vm.editing)"
							disabled="@load(vm.searching)" format="dd/MM/yyyy" hflex="1"
							readonly="@load(not vm.searching)"
							value="@load(vm.saleSummary.quotation.deliveryDate) @save(vm.saleSummary.quotation.deliveryDate)">
						</datebox>
						<space></space>
					</row>
					<row>
						<label sclass="formLabel" value="Obra:"></label>
						<combobox autocomplete="@load(vm.searching)"
							autodrop="@load(vm.searching)" buttonVisible="false"
							maxlength="100" model="@load(vm.listConstruction)"
							onChange="@command(vm.searching ? 'loadSaleSummary':'', field='construction', val=self.value)"
							onFocus="@command(vm.searching ? 'searchSaleSummaryByField':'', field='construction')"
							readonly="@load(not vm.searching ? not vm.editing : 'false')"
							value="@save(vm.saleSummary.construction, before = 'confirmSave') @load(vm.saleSummary.construction) @validator(vm.noEmpty, component = self)"
							width="100%">
						</combobox>
						<label value="Tipo Obra:" sclass="formLabel">
						</label>
						<radiogroup>
							<radio
								checked="@load(vm.saleSummary.quotation eq null ? 'false' : vm.saleSummary.quotation.type)"
								disabled="true" label="Nuevo" value="true">
							</radio>
							<radio
								checked="@load(vm.saleSummary.quotation eq null ? 'false' : not vm.saleSummary.quotation.type)"
								disabled="true" label="Modernizacion" value="false">
							</radio>
						</radiogroup>
						<space></space>
					</row>
					<row>
						<label sclass="formLabel" value="Dirección:"></label>
						<textbox disabled="@load(vm.searching)"
							maxlength="200"
							readonly="@load(not vm.searching ? not vm.editing : 'false')"
							value="@save(vm.saleSummary.constructionAddress, before = 'confirmSave') @load(vm.saleSummary.constructionAddress) @validator(vm.noEmpty, component = self)"
							width="98%">
						</textbox>
						<label sclass="formLabel" value="Vendedor:">
						</label>
						<combobox autocomplete="@load(vm.searching)"
							autodrop="@load(vm.searching)" buttonVisible="false"
							maxlength="100" model="@load(vm.listSeller)"
							onChange="@command(vm.searching ? 'loadSaleSummary':'', field='seller', val=self.value)"
							onFocus="@command(vm.searching ? 'searchSaleSummaryByField':'', field='seller')"
							readonly="@load(not vm.searching)"
							value="@save(vm.saleSummary.quotation.seller, before = 'confirmSave') @load(vm.saleSummary.quotation.seller) @validator(vm.noEmpty, component = self)"
							width="100%">
						</combobox>
						<space></space>
					</row>
					<row>
						<label sclass="formLabel" value="Ciudad:"></label>
						<textbox disabled="@load(vm.searching)"
							maxlength="100"
							readonly="@load(not vm.searching ? not vm.editing : 'false')"
							value="@save(vm.saleSummary.constructionCity, before = 'confirmSave') @load(vm.saleSummary.constructionCity) @validator(vm.noEmpty, component = self)"
							width="98%">
						</textbox>
						<label sclass="formLabel"
							value="Teléfono contacto:">
						</label>
						<textbox disabled="@load(vm.searching)"
							maxlength="30"
							readonly="@load(not vm.searching ? not vm.editing : 'false')"
							value="@save(vm.saleSummary.contactPhone, before = 'confirmSave') @load(vm.saleSummary.contactPhone) @validator(vm.noEmpty, component = self)"
							width="97%">
						</textbox>
						<space></space>
					</row>
					<row>
						<label sclass="formLabel"
							value="Persona contacto:">
						</label>
						<textbox disabled="@load(vm.searching)"
							maxlength="100"
							readonly="@load(not vm.searching ? not vm.editing : 'false')"
							value="@save(vm.saleSummary.contactName, before = 'confirmSave') @load(vm.saleSummary.contactName) @validator(vm.noEmpty, component = self)"
							width="98%">
						</textbox>
						<label sclass="formLabel" value="Correo:"></label>
						<textbox disabled="@load(vm.searching)"
							maxlength="100"
							readonly="@load(not vm.searching ? not vm.editing : 'false')"
							value="@save(vm.saleSummary.email, before = 'confirmSave') @load(vm.saleSummary.email) @validator(vm.noEmail, component = self)"
							width="97%">
						</textbox>
						<space></space>
					</row>
					<row>
						<label sclass="formLabel"
							value="Planos de corte:">
						</label>
						<radiogroup
							selectedItem="@save(vm.saleSummary.planeC)">
							<radio
								checked="@load(vm.saleSummary.planeC)" disabled="true"
								label="Si" value="true">
							</radio>
							<radio
								checked="@load(not vm.saleSummary.planeC)" disabled="true"
								label="No" value="false">
							</radio>
						</radiogroup>
						<label sclass="formLabel"
							value="Planos de planta:">
						</label>
						<radiogroup
							selectedItem="@save(vm.saleSummary.planeP)">
							<radio
								checked="@load(vm.saleSummary.planeP)" disabled="true"
								label="Si" value="true">
							</radio>
							<radio
								checked="@load(not vm.saleSummary.planeP)" disabled="true"
								label="No" value="false">
							</radio>
						</radiogroup>
						<space></space>
					</row>
				</rows>
			</grid>
		</groupbox>
		<groupbox closable="false" mold="3d">
			<caption label="INFORMACION GENERAL" sclass="formLabel">
			</caption>
			<grid>
				<columns>
					<column width="20%"></column>
					<column width="20%"></column>
					<column width="17%"></column>
					<column width="23%"></column>
					<column></column>
				</columns>
				<rows>
					<row>
						<label sclass="formLabel"
							value="Tipo de Edificio:">
						</label>
						<combobox buttonVisible="@load(vm.editing)"
							disabled="@load(vm.searching)"
							model="@load(vm.editing ? vm.listBType : null)"
							readonly="@load(not vm.searching)"
							selectedItem="@bind(vm.saleSummary.basicDataByBuildingType)"
							value="@save(vm.seleccione, before = 'confirmSave') @load(vm.saleSummary.basicDataByBuildingType eq null ? vm.seleccione : vm.saleSummary.basicDataByBuildingType.name) @validator(vm.noSelect, component = self)"
							width="100%">
							<template name="model" var="bType">
								<comboitem label="@load(bType.name)"></comboitem>
							</template>
						</combobox>
						<label sclass="formLabel"
							value="Cantidad de ascensores:">
						</label>
						<intbox disabled="@load(vm.searching)"
							onChanging="@command('updateMotorQuantity')"
							readonly="@load(not vm.searching ? not vm.editing : 'false')"
							value="@save(vm.saleSummary.elevatorQuantity, before = 'confirmSave') @load(vm.saleSummary.elevatorQuantity) @validator(vm.noElevatorQuantity, component = self)"
							width="97%">
						</intbox>
						<space></space>
					</row>
					<row>
						<label sclass="formLabel"
							value="Tipo de Ascensor:">
						</label>
						<combobox buttonVisible="@load(vm.editing)"
							disabled="@load(vm.searching)"
							model="@load(vm.editing ? vm.listElevatorType : null)"
							onChange="@command('selectElevatorType')"
							readonly="@load(not vm.searching)"
							selectedItem="@bind(vm.saleSummary.basicDataByElevatorType)"
							value="@save(vm.seleccione, before = 'confirmSave') @load(vm.saleSummary.basicDataByElevatorType eq null ? vm.seleccione: vm.saleSummary.basicDataByElevatorType.name) @validator(vm.noSelect, component = self)"
							width="100%">
							<template name="model" var="elevatorType">
								<comboitem
									label="@load(elevatorType.name)">
								</comboitem>
							</template>
						</combobox>
						<label sclass="formLabel"
							value="Cantidad de motores:">
						</label>
						<intbox disabled="@load(vm.searching)"
							readonly="@load(not vm.searching ? vm.saleSummary.quotation.type : 'false')"
							width="97%" value="@bind(vm.saleSummary.motorQuantity)">
						</intbox>
						<space></space>
					</row>
					<row>
						<label sclass="formLabel"
							value="Capacidad (Kg - Cap.):">
						</label>
						<combobox buttonVisible="@load(vm.editing)"
							disabled="@load(vm.searching)"
							model="@load(vm.editing ? vm.listElevatorCapa : null)"
							onChange="@command('loadFans')"
							readonly="@load(not vm.searching)"
							selectedItem="@bind(vm.saleSummary.basicDataByElevatorCapacitance)"
							value="@save(vm.seleccione, before = 'confirmSave') @load(vm.saleSummary.basicDataByElevatorCapacitance eq null ? vm.seleccione : vm.saleSummary.basicDataByElevatorCapacitance.name) @validator(vm.noSelect, component = self)"
							width="100%">
							<template name="model" var="elevatorCapa">
								<comboitem
									label="@load(elevatorCapa.name)">
								</comboitem>
							</template>
						</combobox>
						<label sclass="formLabel"
							value="Tracción de Motores:">
						</label>
						<radiogroup
							selectedItem="@save(vm.saleSummary.motorTraction)">
							<radio
								checked="@load(vm.saleSummary.motorTraction eq '1:1' ? 'true':'false')"
								disabled="@load(not vm.editing)" label="1:1" value="1:1">
							</radio>
							<space></space>
							<radio
								checked="@load(vm.saleSummary.motorTraction eq '2:1' ? 'true':'false')"
								disabled="@load(not vm.editing)" label="2:1" value="2:1">
							</radio>
						</radiogroup>
						<space></space>
					</row>
					<row>
						<label sclass="formLabel"
							value="Tipo de Máquina:">
						</label>
						<combobox buttonVisible="@load(vm.editing)"
							disabled="@load(vm.searching)"
							model="@load(vm.editing ? vm.listMachineType : null)"
							readonly="@load(not vm.searching)"
							selectedItem="@bind(vm.saleSummary.basicDataByMachineType)"
							value="@save(vm.seleccione, before = 'confirmSave') @load(vm.saleSummary.basicDataByMachineType eq null ? vm.seleccione : vm.saleSummary.basicDataByMachineType.name) @validator(vm.noSelect, component = self)"
							width="100%">
							<template name="model" var="MachineType">
								<comboitem
									label="@load(MachineType.name)">
								</comboitem>
							</template>
						</combobox>
						<label sclass="formLabel" value="Acceso:"></label>
						<combobox buttonVisible="@load(vm.editing)"
							disabled="@load(vm.searching)"
							model="@load(vm.editing ? vm.listBEmbarque : null)"
							readonly="@load(not vm.searching)"
							selectedItem="@bind(vm.saleSummary.basicDataByAccess)"
							value="@save(vm.seleccione, before = 'confirmSave') @load(vm.saleSummary.basicDataByAccess eq null ? vm.seleccione : vm.saleSummary.basicDataByAccess.name) @validator(vm.noSelect, component = self)"
							width="100%">
							<template name="model" var="Access">
								<comboitem label="@load(Access.name)">
								</comboitem>
							</template>
						</combobox>
						<space></space>
					</row>
					<row>
						<label sclass="formLabel"
							value="Base de Máquina:">
						</label>
						<combobox buttonVisible="@load(vm.editing)"
							disabled="@load(vm.searching)"
							model="@load(vm.editing ? vm.listMachineBase : null)"
							readonly="@load(not vm.searching)"
							selectedItem="@bind(vm.saleSummary.basicDataByMachineBase)"
							value="@load(vm.saleSummary.basicDataByMachineBase eq null ? vm.seleccione : vm.saleSummary.basicDataByMachineBase.name)"
							width="100%">
							<template name="model" var="machineBase">
								<comboitem
									label="@load(machineBase.name)">
								</comboitem>
							</template>
						</combobox>
						<label sclass="formLabel"
							value="Velocidad M/S:">
						</label>
						<combobox buttonVisible="@load(vm.editing)"
							disabled="@load(vm.searching)"
							model="@load(vm.editing ? vm.listSpeed : null)"
							readonly="@load(not vm.searching)"
							selectedItem="@bind(vm.saleSummary.basicDataBySpeed)"
							value="@save(vm.seleccione, before = 'confirmSave') @load(vm.saleSummary.basicDataBySpeed eq null ? vm.seleccione : vm.saleSummary.basicDataBySpeed.name) @validator(vm.noSelect, component = self)"
							width="100%">
							<template name="model" var="Speed">
								<comboitem label="@load(Speed.name)">
								</comboitem>
							</template>
						</combobox>
						<space></space>
					</row>
					<row>
						<label sclass="formLabel"
							value="Tipo de Corriente:">
						</label>
						<combobox buttonVisible="@load(vm.editing)"
							disabled="@load(vm.searching)"
							model="@load(vm.editing ? vm.listElectricityType : null)"
							readonly="@load(not vm.searching)"
							selectedItem="@bind(vm.saleSummary.basicDataByElectricityType)"
							value="@save(vm.seleccione, before = 'confirmSave') @load(vm.saleSummary.basicDataByElectricityType eq null ? vm.seleccione : vm.saleSummary.basicDataByElectricityType.name) @validator(vm.noSelect, component = self)"
							width="100%">
							<template name="model"
								var="ElectricityType">
								<comboitem
									label="@load(ElectricityType.name)">
								</comboitem>
							</template>
						</combobox>
						<label sclass="formLabel"
							value="Tensión de alumbrado:">
						</label>
						<combobox buttonVisible="@load(vm.editing)"
							disabled="@load(vm.searching)"
							model="@load(vm.editing ? vm.listVoltageLighting : null)"
							readonly="@load(not vm.searching)"
							selectedItem="@bind(vm.saleSummary.basicDataByVoltageLighting)"
							value="@load(vm.saleSummary.basicDataByVoltageLighting eq null ? vm.seleccione : vm.saleSummary.basicDataByVoltageLighting.name)"
							width="100%">
							<template name="model"
								var="VoltageLighting">
								<comboitem
									label="@load(VoltageLighting.name)">
								</comboitem>
							</template>
						</combobox>
						<space></space>
					</row>
					<row>
						<label sclass="formLabel" value="Frecuencia:"></label>
						<combobox buttonVisible="@load(vm.editing)"
							disabled="@load(vm.searching)"
							model="@load(vm.editing ? vm.listFrequency : null)"
							readonly="@load(not vm.searching)"
							selectedItem="@bind(vm.saleSummary.basicDataByFrequency)"
							value="@load(vm.saleSummary.basicDataByFrequency eq null ? vm.seleccione : vm.saleSummary.basicDataByFrequency.name)"
							width="100%">
							<template name="model" var="Frequency">
								<comboitem
									label="@load(Frequency.name)">
								</comboitem>
							</template>
						</combobox>
						<label sclass="formLabel"
							value="Secuencia de Paradas:">
						</label>
						<cell colspan="2" rowspan="3">
							<grid>
								<columns>
									<column width="20%"></column>
									<column width="35%"></column>
									<column></column>
								</columns>
								<rows>
									<row>
										<checkbox
											checked="@bind(vm.saleSummary.stopSequenceContinuous)"
											disabled="@load(not vm.editing)" label="Continua:"
											onClick="@command('isStopSequenceContinuous')">
										</checkbox>
										<textbox
											disabled="@load(vm.searching)"
											readonly="@load(not vm.searching ? vm.editing ? not vm.saleSummary.stopSequenceContinuous : 'true' : 'false')"
											maxlength="150"
											value="@load(vm.saleSummary.stopSequenceContinuous ? vm.saleSummary.stopSequenceContinuousQ : '') @save(vm.saleSummary.stopSequenceContinuousQ)"
											width="100%">
										</textbox>
										<div>
											<label style="float: left"
												value="Nro Paradas:" width="40%">
											</label>
											<intbox
												disabled="@load(vm.searching)"
												readonly="@load(not vm.searching ? vm.editing ? not vm.saleSummary.stopSequenceContinuous : 'true' : 'false')"
												style="float: right"
												value="@save(vm.saleSummary.stopSequenceContinuousNumber, before = 'confirmSave') @load(vm.saleSummary.stopSequenceContinuous ? vm.saleSummary.stopSequenceContinuousNumber : '0') @validator(vm.noZeroContinuous, component = self)"
												width="55%">
											</intbox>
										</div>
									</row>
									<row>
										<checkbox
											checked="@load(vm.saleSummary.stopSequenceContinuous ? 'false' : vm.saleSummary.stopSequenceEven) @save(vm.saleSummary.stopSequenceEven)"
											disabled="@load(not vm.editing ? 'true' : vm.saleSummary.stopSequenceContinuous)"
											label="Par">
										</checkbox>
										<textbox
											disabled="@load(vm.searching)"
											readonly="@load(not vm.searching ? vm.editing ? not vm.saleSummary.stopSequenceEven : 'true' : 'false')"
											maxlength="150"
											value="@load(vm.saleSummary.stopSequenceContinuous ? '' : vm.saleSummary.stopSequenceEven ? vm.saleSummary.stopSequenceEvenQ : '') @save(vm.saleSummary.stopSequenceEvenQ)"
											width="100%">
										</textbox>
										<div>
											<label style="float: left"
												value="Nro Paradas:" width="40%">
											</label>
											<intbox
												disabled="@load(vm.searching)"
												readonly="@load(not vm.searching ? vm.editing ? not vm.saleSummary.stopSequenceEven : 'true' : 'false')"
												style="float: right"
												value="@save(vm.saleSummary.stopSequenceEvenNumber, before = 'confirmSave') @load(vm.saleSummary.stopSequenceContinuous ? '0' : vm.saleSummary.stopSequenceEven ? vm.saleSummary.stopSequenceEvenNumber : '0') @validator(vm.noZeroEven, component = self)"
												width="55%">
											</intbox>
										</div>
									</row>
									<row>
										<checkbox
											checked="@load(vm.saleSummary.stopSequenceContinuous ? 'false' : vm.saleSummary.stopSequenceOdd) @save(vm.saleSummary.stopSequenceOdd)"
											disabled="@load(not vm.editing ? 'true' : vm.saleSummary.stopSequenceContinuous)"
											label="Impar">
										</checkbox>
										<textbox
											disabled="@load(vm.searching)"
											readonly="@load(not vm.searching ? vm.editing ? not vm.saleSummary.stopSequenceOdd : 'true' : 'false')"
											maxlength="150"
											value="@load(vm.saleSummary.stopSequenceContinuous ? '' : vm.saleSummary.stopSequenceOdd ? vm.saleSummary.stopSequenceOddQ : '') @save(vm.saleSummary.stopSequenceOddQ)"
											width="100%">
										</textbox>
										<div>
											<label style="float: left"
												value="Nro Paradas:" width="40%">
											</label>
											<intbox
