											</intbox>
											<space></space>
										</row>
										<row>
											<label sclass="formLabel"
												value="Sistel con Flecha">
											</label>
											<hbox>
												<label
													sclass="formLabel" value="PB:">
												</label>
												<checkbox
													checked="@bind(vm.saleSummary.sistelWarrowPb)"
													disabled="@load(not vm.editing ? 'true' : vm.disableSistelHall ? 'true' : 'false')">
												</checkbox>
											</hbox>
											<label sclass="formLabel"
												value="Cantidad Pisos:">
											</label>
											<intbox
												disabled="@load(vm.searching)"
												readonly="@load(not vm.searching ? vm.editing ? not vm.saleSummary.sistelWarrowPb : 'true' : 'false')"
												value="@bind(vm.saleSummary.sistelWarrowFloor)"
												width="97%">
											</intbox>
											<space></space>
