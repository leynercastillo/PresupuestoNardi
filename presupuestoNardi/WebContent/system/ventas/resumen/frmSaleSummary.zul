									onClick="@command('checkWidthDoorFrame', component = cmbBDoorframeType)"
									value="MARCO">
								</radio>
							</radiogroup>
						</hbox>
						<space></space>
					</row>
					<row>
						<label sclass="formLabel"
							value="Botoneras de pasillo:">
