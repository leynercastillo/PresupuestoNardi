<zk>
	<window border="normal" width="70%" height="80%"
		mode="modal" closable="true"
		apply="org.zkoss.bind.BindComposer"
		viewModel="@id('vm') @init('controller.FrmReport')" id="winReport"
		title="@load(vm.reportTitle)" onClose="@command('close')">
		<iframe id="report" style="background-color:transparent"
			width="100%" height="100%" src="@load(vm.report)" />
	</window>
</zk>