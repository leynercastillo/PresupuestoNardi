	<parameter name="number" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT
	number AS 'Numero',
	partnerName AS 'Cliente',
	date AS 'Fecha',
	construction AS 'Construccion',
	CASE WHEN type = TRUE THEN 'Nuevo' ELSE 'Modernización' END AS 'Tipo',
	constructionNumber AS 'Numero Construccion',
	seller AS 'Vendedor',
	constructionAddress AS 'Direccion',
	constructionCity AS 'Ciudad',
	contactPhone AS 'Numero Contacto',
	email AS 'Correo',
	contactName AS 'Persona Contacto',
	CASE WHEN planeP = TRUE THEN 'Si' ELSE 'NO' END AS 'Planos de corte',
	CASE WHEN planeC = TRUE THEN 'SI' ELSE 'NO' END AS 'Planos de planta',
	(SELECT
			name
		FROM
			BasicData
		WHERE
			buildingType = idBasic) AS 'Tipo de edificio',
	elevatorQuantity AS 'Cantidad de ascensores',
	(SELECT
			name
		FROM
			BasicData
		WHERE
			elevatorType = idBasic) AS 'Tipo de ascensor',
	motorQuantity AS 'Cantidad de motores',
	(SELECT
			name
		FROM
			BasicData
		WHERE
			elevatorCapacitance = idBasic) AS 'Carga (Kg - Cap)',
	motorTraction AS 'Planos de corte',
	(SELECT
			name
		FROM
			BasicData
		WHERE
			machineType = idBasic) AS 'Tipo de Maquina',
	(SELECT
			name
		FROM
			BasicData
		WHERE
			embarque = idBasic) AS 'Embarque',
	(SELECT
			name
		FROM
			BasicData
		WHERE
			electricityType = idBasic) AS 'Tipo de Corriente',
	(SELECT
			name
		FROM
			BasicData
		WHERE
			speed = idBasic) AS 'Velocidad M/S',
	(SELECT
			name
		FROM
			BasicData
		WHERE
			frequency = idBasic) AS 'Frecuencia',
	(SELECT
			name
		FROM
			BasicData
		WHERE
			voltageLighting = idBasic) AS 'Tensión de alumbrado',
	(SELECT
			name
		FROM
			BasicData
		WHERE
			hourMachine = idBasic) AS 'Conexiones horas máquina',
	(SELECT
			name
		FROM
			BasicData
		WHERE
			stopSequenceContinuous = idBasic) AS 'Continua',
	stopSequenceContinuousQ AS 'Cantidad',
	(SELECT
			name
		FROM
			BasicData
		WHERE
			stopSequenceEven = idBasic) AS 'Par',
	stopSequenceEvenQ AS 'Cantidad',
	(SELECT
			name
		FROM
			BasicData
		WHERE
			stopSequenceOdd = idBasic) AS 'Impar',
	stopSequenceOddQ AS 'Cantidad',
	(SELECT
			name
		FROM
			BasicData
		WHERE
			maneuverType = idBasic) AS 'Tipo de maniobra',
	stopNumber AS 'Numero de paradas',
	tour AS 'Recorrido',
	onTour AS 'Sobre recorrido',
	widthHole AS 'Ancho de Hueco (mm)',
	fossa AS 'Fosa (mm)',
	bottomHole AS 'Fondo de Hueco (mm)',
	(SELECT
			name
		FROM
			BasicData
		WHERE
			design1357 = idBasic) AS 'Diseño 1357',
	(SELECT
			name
		FROM
			BasicData
		WHERE
			designP26 = idBasic) AS 'Diseño P26',
	CASE WHEN designSpecial = TRUE THEN 'Si' Else 'No' END AS 'Diseño Especial',
	designSpecialComment,
	(SELECT
			name
		FROM
			BasicData
		WHERE
			roofType = idBasic) AS 'Tipo de techo',
	(SELECT
			name
		FROM
			BasicData
		WHERE
			buttonType = idBasic) AS 'Tipo de botonera',
	(SELECT
			name
		FROM
			BasicData
		WHERE
			railing = idBasic) AS 'Pasamano',
	(SELECT
			name
		FROM
			BasicData
		WHERE
			mirror = idBasic) AS 'Espejo',
	(SELECT
			name
		FROM
			BasicData
		WHERE
			floorType = idBasic) AS 'Acabado de piso',
	(SELECT
			name
		FROM
			BasicData
		WHERE
			fan = idBasic) AS 'Ventilador',
	doorOfNumber AS 'Numero de puertas',
	(SELECT
			name
		FROM
			BasicData
		WHERE
			doorType = idBasic) AS 'Tipo de puerta',
	(SELECT
			name
		FROM
			BasicData
		WHERE
			doorSystem = idBasic) AS 'Sistema de puerta',
	(SELECT
			name
		FROM
			BasicData
		WHERE
			doorframeType = idBasic) AS 'Tipo de marco',
	(SELECT
			name
		FROM
			BasicData
		WHERE
			freeAdmission = idBasic) AS 'Entrada libre',
	(SELECT
			name
		FROM
			BasicData
		WHERE
			height = idBasic) AS 'Altura',
	doorFrameStainless AS 'Acero inoxidable',
	doorFrameStainlessDescrip,
	doorFrameBrown AS 'Martillado marrón',
	doorFrameBrownDescrip,
	doorFrameGray AS 'Martillado gris',
	doorFrameGrayDescrip,
	(SELECT
			name
		FROM
			BasicData
		WHERE
			controlType = idBasic) AS 'Tipo de control',
	(SELECT
			name
		FROM
			BasicData
		WHERE
			boothButton = idBasic) AS 'Botonera de Cabina',
	hallButton AS 'Botonera de pasillo',
	sistelWDisplayPB AS 'Sistel con Display PB',
	sistelWDisplayFloor AS 'Sistel con Display Piso',
	sistelWArrowPB AS 'Sistel con Flecha PB',
	sistelWArrowFloor AS 'Sistel con Flecha Piso',
	braile37PB AS 'Chapa de acero con Braille PB',
	braile37Floor AS 'Chapa de acero con Braille Piso',
	antivandalismPB AS 'Chapa de acero con antivandalismo PB',
	antivandalismFLoor AS 'Chapa de acero con antivandalismo Piso',
	(SELECT
			name
		FROM
			BasicData
		WHERE
			boothDisplay = idBasic) AS 'Display Cabina',
	(SELECT
			name
		FROM
			BasicData
		WHERE
			floorDisplay = idBasic) AS 'Display Piso',
	displayPlaceFloor,
	CASE WHEN lightCurtain = TRUE THEN 'Si' ELSE 'NO' END AS 'Cortina luminosa',
	CASE WHEN loadLimiter = TRUE THEN 'Si' ELSE 'NO' END AS 'Limitador de carga',
	CASE WHEN speechSynthesizer = TRUE THEN 'Si' ELSE 'NO' END AS 'Sintetizador de voz',
	CASE WHEN gomSystem = TRUE THEN 'Si' ELSE 'NO' END AS 'Sistema de GOM en Piso',
	CASE WHEN intercom = TRUE THEN 'Si' ELSE 'NO' END AS 'Intercomunicador',
	CASE WHEN phone = TRUE THEN 'Si' ELSE 'NO' END AS 'Teléfono',
	CASE WHEN accessSytem = TRUE THEN 'Si' ELSE 'NO' END AS 'Sistema de acceso',
	CASE WHEN firefighterKeychain = TRUE THEN 'Si' ELSE 'NO' END AS 'Llave de bombero',
	comment AS Comentarios
FROM
	Budget
WHERE
	number = $P{number}]]>
	</queryString>
