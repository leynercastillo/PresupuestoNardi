								checked="@load(vm.quotation.firefighterKeychain ? 'true':'false')"
								disabled="true">
							</radio>
							<radio value="false" label="No"
								checked="@load(vm.quotation.firefighterKeychain ? 'false':'true')"
								disabled="true">
