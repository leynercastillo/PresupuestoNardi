				<columns>
					<column width="15%" align="center"></column>
					<column width="20%"></column>
					<column width="15%"></column>
					<column width="20%"></column>
					<column></column>
				</columns>
				<rows>
					<row>
						<label value="60%" sclass="formLabel"></label>
						<label value="Material Importado:"
							sclass="formLabel">
						</label>
						<doublebox
							value="@bind(vm.quotation.priceImportedMaterial)"
							disabled="true" hflex="1" format="#.##">
						</doublebox>
						<label value="Bs" sclass="formLabel"></label>
						<space></space>
					</row>
					<row>
						<label value="40%" sclass="formLabel"></label>
						<label value="Material Nacional y Mano de Obra:"
							sclass="formLabel">
						</label>
						<doublebox
							value="@bind(vm.quotation.priceNationalMaterial)"
							disabled="true" hflex="1" format="#.##">
						</doublebox>
						<label value="Bs" sclass="formLabel"></label>
						<space></space>
					</row>
					<row>
						<label value="100%" sclass="formLabel"></label>
						<label value="Precio Unitario Total:"
							sclass="formLabel">
						</label>
						<doublebox constraint="no negative"
							value="@bind(vm.quotation.totalPrice)"
							disabled="@load(vm.disabledBudgetNumber ? 'false' : 'true')"
							onBlur="@command('priceUnit', val=self.value)" hflex="1"
							format="#.##">
						</doublebox>
						<label value="Bs" sclass="formLabel"></label>
						<space></space>
					</row>
					<row>
						<label
							value="@load(vm.quotation.elevatorQuantity)"
							sclass="formLabel">
						</label>
						<label
							value="@load(vm.quotation.basicDataByElevatorType eq null ? '' : vm.quotation.basicDataByElevatorType.name)"
							sclass="formLabel">
						</label>
						<doublebox
							value="@load(vm.quotation.totalPrice)" disabled="true" hflex="1"
							format="#.##">
						</doublebox>
						<doublebox
							value="@load(vm.quotation.totalPrice * vm.quotation.elevatorQuantity)"
							disabled="true" hflex="1" format="#.##">
						</doublebox>
						<label value="Bs:" sclass="formLabel"></label>
					</row>
				</rows>
			</grid>
