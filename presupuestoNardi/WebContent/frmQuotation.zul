			<grid>
				<columns>
					<column width="20%"></column>
					<column></column>
				</columns>
				<rows>
					<row>
						<label value="Condiciones de pago:"
							sclass="formLabel">
						</label>
						<textbox width="60%" rows="7"
							disabled="@load(vm.disabledBudgetNumber ? 'false' : 'true')"
							value="@bind(vm.quotation.payment)">
						</textbox>
					</row>
					<row>
						<label value="Garantía:" sclass="formLabel"></label>
						<div>
							<textbox cols="2"
								disabled="@load(vm.disabledBudgetNumber ? 'false' : 'true')"
								value="@bind(vm.quotation.warranty)">
							</textbox>
							<label
								value="meses de garantía. De suscribir contrato de mantenimiento se extiende garantía">
							</label>
							<textbox cols="2"
								disabled="@load(vm.disabledBudgetNumber ? 'false' : 'true')"
								value="@bind(vm.quotation.extendedWarranty)">
							</textbox>
							<label value="meses más."></label>
						</div>
					</row>
					<row>
						<label value="Plazo de entrega:"
							sclass="formLabel">
						</label>
						<div>
							<textbox cols="2"
								disabled="@load(vm.disabledBudgetNumber ? 'false' : 'true')"
								value="@bind(vm.quotation.deliveryEstimate)">
							</textbox>
							<label
								value="meses aproximados despues de la firma del contrato.">
							</label>
						</div>
					</row>
					<row>
						<label value="Válidez del presupuesto:"
							sclass="formLabel">
						</label>
						<div>
							<textbox cols="2"
								disabled="@load(vm.disabledBudgetNumber ? 'false' : 'true')"
								value="@bind(vm.quotation.quotationValidity)">
							</textbox>
							<label
								value="Días a partir de su presentación.">
							</label>
						</div>
					</row>
					<row>
						<label value="Nota:" sclass="formLabel"></label>
						<textbox width="60%" rows="8"
							disabled="@load(vm.disabledBudgetNumber ? 'false' : 'true')"
							value="@bind(vm.quotation.notes)">
						</textbox>
					</row>
				</rows>
			</grid>
		</groupbox>
		<grid>
			<rows>
				<row>
					<hbox>
						<button label="Guardar" sclass="formLabel"
							onClick="@command('save')"
							disabled="@load(vm.disabledBudgetNumber ? 'false' : 'true')"
							image="/resource/images/icons/save.png">
						</button>
						<button label="Imprimir" sclass="formLabel"
							onClick="@command('print')" disabled="@load(vm.disabledPrint)"
							image="/resource/images/icons/print.png">
						</button>
						<button label="Cancelar" sclass="formLabel"
							onClick="@command('restartForm')"
							image="/resource/images/icons/cancel.png">
						</button>
					</hbox>
				</row>
			</rows>
		</grid>
	</window>
</zk>