	</parameter>
	<queryString>
		<![CDATA[SELECT
	(SELECT
			name
		FROM
			BasicData
		WHERE
			controlType = idBasic) AS 'Tipo de control',
	(SELECT
			name
		FROM
			BasicData
		WHERE
			boothButton = idBasic) AS 'Botones de cabina',
	hallButtonPlace AS 'Lugar Botonera',
	CASE WHEN sistelWDisplayPB THEN 'SI' ELSE 'NO' END AS 'Sistel con Display PB',
	sistelWDisplayFloor AS 'Sistel con Display Piso',
	CASE WHEN sistelWArrowPB THEN 'SI' ELSE 'NO' END AS 'Sistel con Flecha PB',
	sistelWArrowFloor AS 'Sistel con Flecha Piso',
	(SELECT
			name
		FROM
			BasicData
		WHERE
			hallButtonType = idBasic) AS 'Tipo de botonera de pasillo',
	(SELECT
			name
		FROM
			BasicData
		WHERE
			hallButton = idBasic) AS 'Boton de pasillo',
	(SELECT
			name
		FROM
			BasicData
		WHERE
			boothDisplay = idBasic) AS 'Display Cabina',
	(SELECT
			name
		FROM
			BasicData
		WHERE
			floorDisplay = idBasic) AS 'Display Piso',
	displayPlaceFloor AS 'Lugar display piso',
	CASE WHEN lightCurtain = TRUE THEN 'Si' ELSE 'NO' END AS 'Cortina luminosa',
	CASE WHEN loadLimiter = TRUE THEN 'Si' ELSE 'NO' END AS 'Limitador de carga',
	CASE WHEN speechSynthesizer = TRUE THEN 'Si' ELSE 'NO' END AS 'Sintetizador de voz',
	CASE WHEN gomSystem = TRUE THEN 'Si' ELSE 'NO' END AS 'Sistema de GOM en Piso',
	CASE WHEN intercom = TRUE THEN 'Si' ELSE 'NO' END AS 'Intercomunicador',
	CASE WHEN phone = TRUE THEN 'Si' ELSE 'NO' END AS 'Teléfono',
	CASE WHEN accessSytem = TRUE THEN 'Si' ELSE 'NO' END AS 'Sistema de acceso',
	CASE WHEN firefighterKeychain = TRUE THEN 'Si' ELSE 'NO' END AS 'Llave de bombero',
	comment AS 'Comentarios',
	seller AS 'Vendedor'
FROM
	Budget
WHERE
	number = $P{number}]]>
	</queryString>
